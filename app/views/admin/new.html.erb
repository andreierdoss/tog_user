<%= error_messages_for :user %>
<% form_for :user, :url => meta_user_create_path,  :html => {:id => "settings"} do |f| -%>
  <h2>Create account</h2>
  <fieldset>

    <%= render :partial => 'form', :locals => { :f => f } %>

    <%= submit_tag 'Create account', {:accesskey => "s"} %>

  </fieldset>
<% end -%>
